<div class="page-title">
    <h1>Contact Us</h1><nav aria-label="You are here:" role="navigation">
      <ul class="breadcrumbs">
        <li>Contact</li>
        <li>
          <span class="show-for-sr">Current: </span> Message Sent
        </li>
      </ul>
    </nav>
</div>


<div class="callout card">
    <div class="callout-header">
        <h4>Submitted Contact Form</h4>
    </div>
    <div class="callout-table">
        
        <table>
            <thead>
                <tr>
                    <th>Value</th>
                    <th>User Submission</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th>Date</th>
                    <td><%= @contact.created_at %></td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td><%= @contact.name %></td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td><%= @contact.email %></td>
                </tr>
                <tr>
                    <th>Message</th>
                    <td><%= @contact.comments %></td>
                </tr>
            </tbody>
        </table>

    </div>
</div>